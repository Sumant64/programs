/*
 * program to print the pattern:
 * 			*
 * 		   * *
 *        * * *
 *       * * * *
 *  */

package lecture1;

public class Program_3_print_basic_pattern {
	public static void main(String[] args) {
		System.out.println("    *");
		System.out.println("**");
		System.out.println("***");
		System.out.println("****");
	}
}
